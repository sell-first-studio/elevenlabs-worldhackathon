# =============================================================================
# Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your values
# NEVER commit your .env file to version control
# =============================================================================

# -----------------------------------------------------------------------------
# Application Settings
# -----------------------------------------------------------------------------
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# -----------------------------------------------------------------------------
# Twilio Configuration (SMS & Voice)
# https://console.twilio.com/
# -----------------------------------------------------------------------------
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=+1234567890

# -----------------------------------------------------------------------------
# ElevenLabs Configuration (Voice Synthesis)
# https://elevenlabs.io/app/settings/api-keys
# -----------------------------------------------------------------------------
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# -----------------------------------------------------------------------------
# LiveKit Configuration (Real-time Communication)
# https://cloud.livekit.io/
# -----------------------------------------------------------------------------
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key_here
LIVEKIT_API_SECRET=your_livekit_api_secret_here

# -----------------------------------------------------------------------------
# BlackBox AI Configuration (Chat Completions)
# https://www.blackbox.ai/
# -----------------------------------------------------------------------------
BLACKBOXAI_API_KEY=your_blackboxai_api_key_here
